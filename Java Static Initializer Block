import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int A,H,B;
            B = scan.nextInt();
            H = scan.nextInt();
        if(B>0 && H>0){
            A = H*B;
            System.out.println(A);
        }else{
            System.out.println("java.lang.Exception: Breadth and height must be positive");
        }
    }
}
